{
    "manifest_version": 2,
    "name": "No History Tracker",
    "version": "1.0",
    "description": "Extension to disable the tracking of URLs of browser histories.",
    "author": "keiwsh",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true
    },
    "permissions": [
        "*://*/*",
        "history"
    ]
}
